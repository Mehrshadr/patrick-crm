    {
      "parameters": {
        "jsCode": "// Timezone settings\nconst TIMEZONE = 'America/Toronto';\n\n// Helper to get time in Toronto\nfunction getTorontoTime(date) {\n  return new Date(date.toLocaleString('en-US', { timeZone: TIMEZONE }));\n}\n\n// Helper to create a target date in Toronto Timezone\nfunction createTargetDate(baseDate, daysToAdd, hour) {\n  const target = new Date(baseDate);\n  target.setDate(target.getDate() + daysToAdd);\n  target.setHours(hour, 0, 0, 0);\n  return target;\n}\n\nconst nowToronto = getTorontoTime(new Date());\n\nreturn items.map(item => {\n  const lead = item.json;\n  const status = lead.Status;\n  const dateAddedStr = lead['Date Added'];\n  \n  let action = 'none';\n\n  if (dateAddedStr) {\n    // Safety: Replace ' ' with 'T' for ISO parsing if needed\n    const safeDateStr = dateAddedStr.replace(' ', 'T');\n    const dateAdded = new Date(safeDateStr);\n    const leadTimeToronto = getTorontoTime(dateAdded);\n    \n    // Determine Start Hour of the Lead (0-23)\n    const leadHour = leadTimeToronto.getHours();\n    \n    let targetS2, targetS3, targetFunnel;\n\n    if (leadHour >= 8) {\n       // Scenario A: Day Time Lead (8 AM - 11:59 PM)\n       // Stage 2: Next Day (Day+1) at 2:00 PM (14:00)\n       // Stage 3: Day After Next (Day+2) at 7:00 PM (19:00)\n       targetS2 = createTargetDate(leadTimeToronto, 1, 14);\n       targetS3 = createTargetDate(leadTimeToronto, 2, 19);\n    } else {\n       // Scenario B: Night Time Lead (12 AM - 7:59 AM)\n       // Stage 2: Same Day (Day+0) at 12:00 PM (12:00)\n       // Stage 3: Next Day (Day+1) at 7:00 PM (19:00)\n       targetS2 = createTargetDate(leadTimeToronto, 0, 12);\n       targetS3 = createTargetDate(leadTimeToronto, 1, 19);\n    }\n\n    // Funnel: 24h after Stage 3 (approx Day+3 or Day+2 at 7 PM)\n    targetFunnel = new Date(targetS3);\n    targetFunnel.setDate(targetFunnel.getDate() + 1);\n\n    // Check Triggers\n    // Logic: If NOW >= TargetTime, we are eligible.\n    // Since status updates prevent duplicate sending, we just check if we passed the milestone.\n\n    if (status === 'New Lead - Welcome Sent') {\n       if (nowToronto >= targetS2) {\n         action = 'step2';\n       }\n    } else if (status === 'New Lead - Welcome Sent 2') {\n       if (nowToronto >= targetS3) {\n         action = 'step3';\n       }\n    } else if (status === 'New Lead - Welcome Sent 3') {\n       if (nowToronto >= targetFunnel) {\n         action = 'ghosted';\n       }\n    }\n  }\n\n  return {\n    json: {\n      ...lead,\n      nurture_action: action\n    }\n  };\n});"
      },
